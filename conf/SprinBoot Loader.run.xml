<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="SprinBoot Loader" type="SpringBootApplicationConfigurationType"
                   factoryName="Spring Boot">
        <option name="ACTIVE_PROFILES"/>
        <option name="ALTERNATIVE_JRE_PATH" value="17"/>
        <option name="ALTERNATIVE_JRE_PATH_ENABLED" value="true"/>
        <option name="DEBUG_MODE" value="true"/>
        <module name="pizzahut"/>
        <option name="SPRING_BOOT_MAIN_CLASS" value="fr.epsi.rennes.poec.stephen.mistayan.PizzaApplication"/>
        <extension name="coverage" sample_coverage="true">
            <include resource="org/springframework/boot/logging/logback/default.xml"/>
            <include resource="org/springframework/boot/logging/logback/console-appender.xml"/>
            <root level="INFO">
                <appender-ref ref="CONSOLE"/>
            </root>
            <logger name="org.springframework.web" level="DEBUG"/>

            <pattern>
                <option name="PATTERN" value="fr.epsi.rennes.poec.stephen.mistayan.dao.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
            <pattern>
                <option name="PATTERN" value="fr.epsi.rennes.poec.stephen.mistayan.service.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
            <pattern>
                <option name="PATTERN" value="fr.epsi.rennes.poec.stephen.mistayan.Controller.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>
